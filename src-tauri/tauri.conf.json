{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "oversync",
  "version": "0.1.3",
  "identifier": "com.oversync.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Oversync",
        "width": 1000,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": [],
    "copyright": "",
    "category": "Utility",
    "shortDescription": "",
    "longDescription": ""
  },
  "plugins": {
    "fs": {
      "scope": ["$APP_DATA/*", "$HOME/*"]
    },
    "shell": {
      "open": true
    },
    "dialog": {},
    "os": {},
    "process": {},
    "opener": {}
  }
}
